    "knex": "^0.95.15",
    "lodash": "^4.17.21",
    "pg": "^8.7.1",
    "pgtools": "^0.3.2",
    "webpack": "^5.65.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5"
  }
